{
	"name": "repo-trillo",
	"version": "1.0.0",
	"description": "Landing page for Trillo",
	"main": "index.js",
	"scripts": {
		"watch:sass": "node-sass sass/main.scss css/style.css -w",
		"compile:sass": "sass  sass/main.scss css/style.css -w",

		"concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
		"prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css",
		"compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
		"build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ghozlene/Responsive-website-trillo.git"
	},
	"keywords": [
		"html",
		"css",
		"sass",
		"scss"
	],
	"author": "GHOZLENE mohamed Achref",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/ghozlene/Responsive-website-trillo.git/issues"
	},
	"homepage": "https://github.com/ghozlene/Responsive-website-trillo.git#readme",
	"devDependencies": {
		"sass": "^1.55.0"
	}
}
